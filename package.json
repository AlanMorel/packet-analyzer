        "@storybook/react": "^6.5.15",
        "@storybook/testing-library": "^0.0.13",
        "@swc/jest": "^0.2.24",
        "@types/jest": "^29.2.5",
        "@types/node": "^18.11.18",
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.10",
        "@vitejs/plugin-react": "^3.0.0",
        "cross-env": "^7.0.3",
        "dotenv": "^16.0.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
