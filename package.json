    "license": "MIT",
    "author": "Alan Morel",
    "type": "module",
    "scripts": {
        "build": "tsc && vite build",
        "ts:check": "tsc --noEmit",
        "dev": "vite",
        "lint": "eslint src/**/*.{js,jsx,ts,tsx,json} --fix",
        "preview": "vite preview",
        "storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006",
