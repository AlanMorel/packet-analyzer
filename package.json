        "stylelint": "stylelint src/**/*.{css,scss} --fix",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage"
    },
    "dependencies": {
        "@storybook/addon-actions": "^6.5.15",
        "@storybook/addon-essentials": "^6.5.15",
